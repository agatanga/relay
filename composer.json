        }
    ],
    "require": {
        "php": "^8.0",
        "illuminate/bus": "^8.0|^9.0|^10.0",
        "illuminate/support": "^7.14|^8.0|^9.0|^10.0",
        "laravel/serializable-closure": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Agatanga\\Relay\\": "src"
